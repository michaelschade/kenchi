{
  "extends": [
    "config:base",
    "group:recommended",
    "group:definitelyTyped",
    "schedule:nonOfficeHours"
  ],
  "lockFileMaintenance": {
    "enabled": true
  },
  "ignoreDeps": [
    "@types/graphql-upload",
    "@babel/*"
  ],
  "packageRules": [
    {
      "matchDepTypes": [
        "devDependencies"
      ],
      "matchPackagePatterns": [
        "lint",
        "prettier"
      ],
      "automerge": true
    },
    {
      "matchPackagePatterns": ["@bull-board/*"],
      "groupName": "@bull-board"
    },
    {
      "matchPackagePatterns": ["slate", "slate-*"],
      "groupName": "slate"
    }
  ],
  "hostRules": [
    {
      "matchHost": "npm.pkg.github.com",
      "hostType": "npm",
      "encrypted": {
        "token": ""
      }
    },
    {
      "matchHost": "npm.fontawesome.com",
      "hostType": "npm",
      "token": "264E9B96-1535-4EEB-B97E-F7E324F74805"
    }
  ],
  "enabled": false
}
