{
  "name": "@kenchi/backend",
  "description": "Like backend, but with more TS",
  "license": "MIT",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "prepare": "$npm_execpath generate",
    "clean": "rm -rf build",
    "start": "nodemon -r dotenv/config --transpile-only ./api/app.ts",
    "start:admin": "ADMIN=true nodemon -r dotenv/config --transpile-only ./api/app.ts",
    "start:worker": "nodemon -r dotenv/config --transpile-only ./api/queue/worker.ts",
    "start:inspect": "nodemon -r dotenv/config --transpile-only --inspect=localhost:9229 ./api/app.ts",
    "tsc": "tsc --noEmit",
    "lint": "eslint '**/*.{js,ts,tsx}'",
    "test": "jest",
    "migrate": "node-pg-migrate --tsconfig=./tsconfig.migrate.json --migration-filename-format=utc --ignore-pattern='WIP_.*'",
    "generate": "$npm_execpath generate:prisma && $npm_execpath generate:nexus",
    "generate:nexus": "NEXUS_SHOULD_GENERATE_ARTIFACTS=true ts-node -r dotenv/config --transpile-only ./api/graphql",
    "generate:prisma": "prisma generate",
    "build": "tsc -p tsconfig.build.json",
    "build:staging": "$npm_execpath build:production # same since all env vars are in Render",
    "build:production": "export SENTRY_VERSION=$SERVICE@$(sentry-cli releases propose-version) && $npm_execpath build && ./deploy-render",
    "serve": "SENTRY_VERSION=$SERVICE@$(sentry-cli releases propose-version) node build/api/app",
    "serve:admin": "SENTRY_VERSION=$SERVICE@$(sentry-cli releases propose-version) ADMIN=true node build/api/app",
    "serve:worker": "SENTRY_VERSION=$SERVICE@$(sentry-cli releases propose-version) node build/api/queue/worker"
  },
  "dependencies": {
    "@algolia/client-search": "4.13.1",
    "@bull-board/api": "3.11.1",
    "@bull-board/express": "3.11.1",
    "@kenchi/root": "workspace:*",
    "@kenchi/shared": "workspace:*",
    "@kenchi/slate-tools": "workspace:*",
    "@prisma/client": "3.15.1",
    "@sentry/node": "7.7.0",
    "@sentry/tracing": "7.7.0",
    "@types/body-parser": "1.19.2",
    "@types/bull": "3.15.8",
    "@types/cli-progress": "3.11.0",
    "@types/cookie-parser": "1.4.3",
    "@types/cors": "2.8.12",
    "@types/csurf": "1.11.2",
    "@types/debug": "4.1.7",
    "@types/express": "4.17.13",
    "@types/express-serve-static-core": "4.17.28",
    "@types/express-session": "1.17.4",
    "@types/graphql-upload": "8.0.11",
    "@types/jsdom": "16.2.14",
    "@types/lodash": "4.14.182",
    "@types/luxon": "2.3.2",
    "@types/node": "16.11.39",
    "@types/node-fetch": "2.6.1",
    "@types/on-finished": "2.3.1",
    "@types/request": "2.48.8",
    "@types/tmp": "0.2.3",
    "@types/uid-safe": "2.1.2",
    "algoliasearch": "4.13.1",
    "apollo-server-core": "3.8.2",
    "apollo-server-express": "3.8.2",
    "aws-sdk": "2.1152.0",
    "body-parser": "1.20.0",
    "bull": "4.8.3",
    "chalk": "4.1.2",
    "cli-progress": "3.11.1",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "crypto-random-string": "3.3.1",
    "csurf": "1.11.0",
    "customerio-node": "3.3.2",
    "dataloader": "2.1.0",
    "debug": "4.3.4",
    "dotenv": "10.0.0",
    "express": "4.17.3",
    "express-async-errors": "3.1.1",
    "express-session": "1.17.3",
    "fast-deep-equal": "3.1.3",
    "get-port": "5.1.1",
