{
  "name": "@kenchi/frontend",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "BROWSER=none craco start",
    "build": "INLINE_RUNTIME_CHUNK=false craco build",
    "build:playwright": "cp .env.test .env.production.local && $npm_execpath build && rm .env.production.local",
    "build:staging": "export REACT_APP_SENTRY_VERSION=frontend@$(sentry-cli releases propose-version) && env-cmd -f .env.staging $npm_execpath build && ./deploy-render",
    "build:production": "export REACT_APP_SENTRY_VERSION=frontend@$(sentry-cli releases propose-version) && $npm_execpath build && ./deploy-render",
    "generate": "$npm_execpath generate:graphql",
    "generate:graphql": "graphql-codegen",
    "test": "craco test",
    "test:debug": "craco --inspect-brk test --runInBand --no-cache",
    "lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "eject": "craco eject",
    "playwright:inspector": "PWDEBUG=1 playwright test",
    "playwright:debug": "PWDEBUG=console playwright test",
    "playwright:install:default-browsers": "playwright install chromium",
    "playwright:install:ci-browsers": "playwright install --with-deps chromium"
  },
  "browserslist": {
    "production": [
      "last 5 chrome versions"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "babelMacros": {
    "twin": {
      "config": "../tailwind.config.js"
    }
  },
