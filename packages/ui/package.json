{
  "name": "@kenchi/ui",
  "private": true,
  "version": "0.1.0",
  "repository": "git://github.com/michaelschade/kenchi.git",
  "scripts": {
    "prepare": "$npm_execpath build",
    "clean": "run-p clean:*",
    "clean:build": "rm -rf lib",
    "start": "run-p start:*",
    "start:babel": "$npm_execpath build:babel --watch",
    "start:tsc": "$npm_execpath build:tsc --watch --preserveWatchOutput",
    "build": "run-p build:*",
    "build:babel": "babel src -d lib --extensions '.ts,.tsx' --ignore '**/*.test.tsx','**/*.test.ts'",
    "build:tsc": "tsc --emitDeclarationOnly",
    "lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "test": "jest"
  },
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "babelMacros": {
    "twin": {
      "config": "../tailwind.config.js"
    }
  },
