{
  "name": "Kenchi",
  "description": "Kenchi is the best way to scale your support team.",
  "version": "0.27",
  "web_accessible_resources": [
    "iframe.html",
    "images/icon-40.png"
  ],
  "permissions": [
    "<all_urls>",
    "tabs",
    "clipboardRead",
    "clipboardWrite",
    "contextMenus",
    "webRequest",
    "webNavigation"
  ],
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {},
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "40": "images/icon-40.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png",
    "200": "images/icon-200.png"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'; frame-src {{APP_HOST}}",
  "commands": {
    "activate": {
      "suggested_key": {
        "default": "Ctrl+Space",
        "mac": "MacCtrl+Space"
      },
      "description": "Show Kenchi"
    },
    "hide": {
      "suggested_key": {
        "default": "Ctrl+Shift+Space",
        "mac": "MacCtrl+Shift+Space"
      },
      "description": "Hide Kenchi"
    }
  },
  "externally_connectable": {
    "matches": [
      "{{APP_HOST}}/*"
    ]
  },
  "manifest_version": 2,
  "key": ""
}
